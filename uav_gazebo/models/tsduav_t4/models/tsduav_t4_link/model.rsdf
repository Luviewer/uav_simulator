<?xml version="1.0" ?>
  <%

# ----------------------------------------------------------------
platform_base_dx = 0
platform_base_dy = 0
platform_base_dz = 0.0

platform_base_mass = 10.5

platform_base_ixx  = 1.4
platform_base_iyy  = 1.0
platform_base_izz  = 0.7

# ----------------------------------------------------------------
link_base_dx = 0;
link_base_dy = 0;
link_base_dz = -0.1045

link_base_mass = 0.2

link_base_ixx  = 0.01
link_base_iyy  = 0.01
link_base_izz  = 0.01

# ----------------------------------------------------------------
link_pipe_dx =  -0.0
link_pipe_dy =  0.243
link_pipe_dz =  -0.1995
link_pipe_mass = 0.5

link_pipe_ixx  = 0.02
link_pipe_iyy  = 0.03
link_pipe_izz  = 0.01

# ----------------------------------------------------------------
saw_dz = -0.304
saw_dx = 1.307
saw_mass = 1.5

saw_ixx  = 0.1
saw_iyy  = 0.1
saw_izz  = 0.1

%>
  <sdf version="1.5">
    <model name="tsduav_t4_link">
      <%# ====================================================================================== %>
      <link name="link_base">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass><%= link_base_mass %></mass>
        <inertia>
          <ixx><%= link_base_ixx %></ixx>
          <iyy><%= link_base_iyy %></iyy>
          <izz><%= link_base_izz %></izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="link_base_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.655  0.255</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <ode>
              <max_vel>100.0</max_vel>
              <min_depth>0.001</min_depth>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="link_base_visual">
        <geometry>
          <mesh>
            <uri>model://tsduav_t4_link/meshes/link_base.STL</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Yellow</name>
          </script>
        </material>
      </visual>
    </link>
    <link name="link_pipe_1">
    <pose><%=link_pipe_dx%> <%=link_pipe_dy%> <%=link_pipe_dz%> 0 <%= -Math::PI/2 %> 0</pose>
    <inertial>
      <pose>0 0 0 0 0 0</pose>
      <mass><%= link_pipe_mass %></mass>
      <inertia>
        <ixx><%= link_pipe_ixx %></ixx>
        <iyy><%= link_pipe_iyy %></iyy>
        <izz><%= link_pipe_izz %></izz>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyz>0</iyz>
      </inertia>
    </inertial>
    <collision name="link_pipe_1_collision">
      <pose>0 0 -0.65 0 0 0</pose>
      <geometry>
        <cylinder>
          <length>1.35</length>
          <radius>0.0275</radius>
        </cylinder>
      </geometry>
      <surface>
        <contact>
          <ode>
            <max_vel>100.0</max_vel>
            <min_depth>0.001</min_depth>
          </ode>
        </contact>
        <friction>
          <ode>
            <mu>100000.0</mu>
            <mu2>100000.0</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name="link_pipe_1_visual">
      <geometry>
        <mesh>
          <uri>model://tsduav_t4_link/meshes/link_pipe.STL</uri>
        </mesh>
      </geometry>
      <material>
        <script>
          <name>Gazebo/Green</name>
        </script>
      </material>
    </visual>
  </link>
  <joint name='link_pipe_1_joint' type="revolute">
    <parent>link_base</parent>
    <child>link_pipe_1</child>
    <axis>
      <xyz>0 1 0</xyz>
      <limit>
        <lower><%= -Math::PI/2 %></lower>
        <upper><%=  Math::PI/2 %></upper>
      </limit>
      <dynamics>
        <damping>0.00001</damping>
      </dynamics>
      <use_parent_model_frame>1</use_parent_model_frame>
    </axis>
  </joint>
  <%# ====================================================================================== %>
  <link name="link_pipe_2">
  <pose><%=link_pipe_dx%> <%=-link_pipe_dy%> <%=link_pipe_dz%> 0 <%= -Math::PI/2 %> 0</pose>
  <inertial>
    <pose>0 0 0 0 0 0</pose>
    <mass><%= link_pipe_mass %></mass>
    <inertia>
      <ixx><%= link_pipe_ixx %></ixx>
      <iyy><%= link_pipe_iyy %></iyy>
      <izz><%= link_pipe_izz %></izz>
      <ixy>0</ixy>
      <ixz>0</ixz>
      <iyz>0</iyz>
    </inertia>
  </inertial>
  <collision name="link_pipe_2_collision">
    <pose>0 0 -0.65 0 0 0</pose>
    <geometry>
      <cylinder>
        <length>1.35</length>
        <radius>0.0275</radius>
      </cylinder>
    </geometry>
    <surface>
      <contact>
        <ode>
          <max_vel>100.0</max_vel>
          <min_depth>0.001</min_depth>
        </ode>
      </contact>
      <friction>
        <ode>
          <mu>100000.0</mu>
          <mu2>100000.0</mu2>
        </ode>
      </friction>
    </surface>
  </collision>
  <visual name="link_pipe_2_visual">
    <geometry>
      <mesh>
        <uri>model://tsduav_t4_link/meshes/link_pipe.STL</uri>
      </mesh>
    </geometry>
    <material>
      <script>
        <name>Gazebo/Green</name>
      </script>
    </material>
  </visual>
</link>
<joint name='link_pipe_2_joint' type="revolute">
  <parent>link_base</parent>
  <child>link_pipe_2</child>
  <axis>
    <xyz>0 1 0</xyz>
    <limit>
      <lower><%= -Math::PI/2 %></lower>
      <upper><%=  Math::PI/2 %></upper>
    </limit>
    <dynamics>
      <damping>0.00001</damping>
    </dynamics>
    <use_parent_model_frame>1</use_parent_model_frame>
  </axis>
</joint>
</model>
</sdf>
