<?xml version="1.0" ?>
  <%

prop_dx = 0.53541
prop_dy = 1.00941
prop_dz = 0.0775
prop_fu_d = -prop_dz - 0.02

prop_timeConstantUp = 0.0125
prop_timeConstantDown = 0.025
prop_maxRotVelocity = 1000
prop_motorConstant = 8.599e-5
prop_momentConstant = 0.25

prop_motors={
	"0" => {:jointName => "prop_0_joint", :linkName => "prop_0", :dir => "ccw", :x =>  prop_dx, :y => -prop_dy, :z => prop_dz},
	"1" => {:jointName => "prop_1_joint", :linkName => "prop_1", :dir => "cw",  :x =>  prop_dx, :y => -prop_dy, :z => prop_fu_d},
	"2" => {:jointName => "prop_2_joint", :linkName => "prop_2", :dir => "cw",  :x => -prop_dx, :y => -prop_dy, :z => prop_dz},
	"3" => {:jointName => "prop_3_joint", :linkName => "prop_3", :dir => "ccw", :x => -prop_dx, :y => -prop_dy, :z => prop_fu_d},

	"4" => {:jointName => "prop_4_joint", :linkName => "prop_4", :dir => "ccw", :x => -prop_dx, :y =>  prop_dy, :z => prop_dz},
	"5" => {:jointName => "prop_5_joint", :linkName => "prop_5", :dir => "cw",  :x => -prop_dx, :y =>  prop_dy, :z => prop_fu_d},
	"6" => {:jointName => "prop_6_joint", :linkName => "prop_6", :dir => "cw",  :x =>  prop_dx, :y =>  prop_dy, :z => prop_dz},
	"7" => {:jointName => "prop_7_joint", :linkName => "prop_7", :dir => "ccw", :x =>  prop_dx, :y =>  prop_dy, :z => prop_fu_d},
}

link_base_dx = 1.034
link_base_dy = 0
link_base_dz = -0.226

saw_dx = 1.6
saw_dy = 0
saw_dz = -0.226

%>
  <sdf version="1.5">
    <model name="tsduav_t4">
      <self_collide>0</self_collide>
      <%# ====================================================================================== %>
      <include>
        <uri>model://tsduav_t4_base</uri>
        <name>tsduav_t4_base</name>
        <pose>0 0 0.0 0 0 0</pose>
      </include>
      <%
			prop_motors.keys.each do |k|
				dir = prop_motors[k][:dir]
        dx = prop_motors[k][:x]
        dy = prop_motors[k][:y]
        dz = prop_motors[k][:z]
      %>
      <include>
        <uri>model://tsduav_t4_prop</uri>
        <name><%= "tsduav_t4_prop_" + k%></name>
        <pose><%= dx %> <%= dy %> <%= dz %> 0 0 0</pose>
      </include>
      <joint name=<%= '"' + 'tsduav_t4_prop_' + k + '_joint' + '"'%> type="revolute">
        <parent>tsduav_t4_base::base</parent>
        <child><%= 'tsduav_t4_prop_' + k + '::prop '%></child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1e+16</lower>
            <upper>1e+16</upper>
          </limit>
          <dynamics>
            <damping>0.00001</damping>
          </dynamics>
          <use_parent_model_frame>1</use_parent_model_frame>
        </axis>
      </joint>
      <plugin name=<%= '"tsduav_t4_prop_'+ k + '_plugin"' %> filename='librotors_gazebo_motor_model.so'>
        <robotNamespace></robotNamespace>
        <jointName><%= 'tsduav_t4_prop_' + k + '_joint' %> </jointName>
        <linkName><%= 'tsduav_t4_prop_' + k + '::prop'%></linkName>
        <turningDirection><%= dir %></turningDirection>
        <timeConstantUp><%= prop_timeConstantUp %></timeConstantUp>
        <timeConstantDown><%= prop_timeConstantDown %></timeConstantDown>
        <maxRotVelocity><%= prop_maxRotVelocity %></maxRotVelocity>
        <motorConstant><%= prop_motorConstant %></motorConstant>
        <momentConstant><%= prop_momentConstant %></momentConstant>
        <commandSubTopic>/gazebo/command/prop_speed</commandSubTopic>
        <motorNumber><%= k %></motorNumber>
        <rotorDragCoefficient>0.000806428</rotorDragCoefficient>
        <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
        <motorSpeedPubTopic><%= 'prop_speed/' + k %></motorSpeedPubTopic>
        <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
      </plugin>
    <% end %>
    <include>
      <uri>model://tsduav_t4_link</uri>
      <name>tsduav_t4_link</name>
      <pose><%=link_base_dx%> 0 <%=link_base_dz%> 0 <%= -Math::PI/2 %> 0</pose>
    </include>
    <joint name='link_base_joint' type='revolute'>
      <parent>tsduav_t4_link::link_base</parent>
      <child>tsduav_t4_base::base</child>
      <pose>0 0 -0.226 0 <%= -Math::PI/2 %> 0</pose>
      <axis>
          <xyz>0 -1 0</xyz>
          <limit>
            <lower><%= -Math::PI %></lower>
            <upper><%= Math::PI/2 %></upper>
          </limit>
          <dynamics>
            <damping>0.001</damping>
          </dynamics>
          <use_parent_model_frame>1</use_parent_model_frame>
        </axis>
    </joint>
    <%# ====================================================================================== %>
    <include>
      <uri>model://tsduav_t4_saw</uri>
      <name>tsduav_t4_saw</name>
      <pose><%=saw_dx%> <%=saw_dy%> <%=saw_dz%> 0 0 0</pose>
    </include>
    <joint name='tsduav_t4_saw_1_joint' type='fixed'>
      <child>tsduav_t4_saw::saw</child>
      <parent>tsduav_t4_link::link_base</parent>
      <use_parent_model_frame>1</use_parent_model_frame>
    </joint>

    <plugin name="ArduRotorQuadOctPlugin" filename="libArduRotorQuadOctPlugin.so">
      <fdm_addr>127.0.0.1</fdm_addr>
      <fdm_port_in>9002</fdm_port_in>
      <fdm_port_out>9003</fdm_port_out>
      <modelXYZToAirplaneXForwardZDown>0 0 0 3.141593 0 0</modelXYZToAirplaneXForwardZDown>
      <gazeboXYZToNED>0 0 0 3.141593 0 0</gazeboXYZToNED>
      <motor_num>8</motor_num>
      <motor_pub>/gazebo/command/prop_speed</motor_pub>
      <imuName>tsduav_t4::tsduav_t4_base::imu_link::imu_sensor</imuName>
      <connectionTimeoutMaxCount>5</connectionTimeoutMaxCount>
    </plugin>
  </model>
</sdf>
